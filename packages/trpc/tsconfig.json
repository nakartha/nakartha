{
  "extends": "@workspace/typescript-config/react-library.json",
  "include": [
    ".",
    "../auth/src/context/context.ts"
  ],
  "paths": {
    "@workspace/prisma": [
      "../prisma"
    ],
    "@workspace/prisma/*": [
      "../prisma/*"
    ],
    "@workspace/auth": [
      "../auth"
    ],
    "@workspace/auth/*": [
      "../auth/*"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node"
  }
}